#! groovy

node {
	
  // Get the source code
 stage('checkout') {
    checkout([$class: 'GitSCM', branches: [[name: '*/dev']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/rajdeva2009/Cub.git']]])
  }
  
  // Build the code
 stage('build') {
	 echo "Build"
   }
	
 // test the code
 stage('test') {
	 echo "test"
   }

// upload to artifact
 stage('upload') {
	 echo "upload"
   }
// deploy code
 stage('deploy') {
	 echo "deploy"
	}

}
  
